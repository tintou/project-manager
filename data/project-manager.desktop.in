[Desktop Entry]
Name=@APP_NAME@
Comment=@RELEASE_NAME@
Exec=@EXEC_NAME@ %U
Icon=@DESKTOP_ICON@
Keywords=install;uninstall;remove;catalogue;store;apps;updates;software;
Terminal=false
Type=Application
StartupNotify=true
Categories=GNOME;GTK;System;PackageManager;
X-GNOME-Gettext-Domain=@GETTEXT_PACKAGE@
Actions=AboutDialog;

[Desktop Action AboutDialog]
Name=About @APP_NAME@
Exec=@EXEC_NAME@ --about
