[Desktop Entry]
Name=@APP_NAME@
Comment=@RELEASE_NAME@
Exec=@EXEC_NAME@ %U
Icon=@DESKTOP_ICON@
Keywords=developer;api;github;launchpad;git;bzr;bazaar;project;code;
Terminal=false
Type=Application
StartupNotify=true
Categories=GNOME;GTK;Development;
X-GNOME-Gettext-Domain=@GETTEXT_PACKAGE@
Actions=AboutDialog;

[Desktop Action AboutDialog]
Name=About @APP_NAME@
Exec=@EXEC_NAME@ --about
